@model List<LavaCarProject.Models.sp_Retorna_Vehiculos_sin_Cliente_Result>
    @{
        ViewBag.Title = "VehiculosLibres";
    }
@section Style{
    <link href="https://kendo.cdn.telerik.com/2020.2.617/styles/kendo.common.min.css" rel="stylesheet" />
    <link href="https://kendo.cdn.telerik.com/2020.2.617/styles/kendo.blueopal.min.css" rel="stylesheet" />
}
<h2>Asignación de Vehículos</h2>

@*<div id="divVehiculosLibres">*@
@*</div>*@
<form class="form" id="fmrVehiculosLibres" method="POST">

            <table class="table">
                <tr>
                    <th>Placa</th>
                    <th>Marca</th>
                    <th>Modelo</th>
                    <th>Tipo Vehículo</th>
                    <th>Número de puertas</th>
                    <th>Número de ruedas</th>
                    <th>Año</th>
                    <th>Acciones</th>
                </tr>
                @{
                    foreach (LavaCarProject.Models.sp_Retorna_Vehiculos_sin_Cliente_Result posicionModelo in this.Model)
                    {
                        <tr>
                            <td>@posicionModelo.placa</td>
                            <td>@posicionModelo.nombre_marca</td>
                            <td>@posicionModelo.nombre_modelo</td>
                            <td>@posicionModelo.nombre_vehiculo</td>
                            <td>@posicionModelo.cantidad_puertas</td>
                            <td>@posicionModelo.cantidad_ruedas</td>
                            <td>@posicionModelo.año_fabricacion</td>
                            <td>
                                <a href="~/Vehiculos/AsignarVehiculos?id_vehiculo=@posicionModelo.id_vehiculo&pplaca=@posicionModelo.placa">Asignar Vehículo</a>
                            </td>
                        </tr>
                    }
                }
            </table>
        </form>
    </div>
@section scripts{
    <script src="https://kendo.cdn.telerik.com/2020.2.617/js/kendo.all.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2020.2.617/js/jszip.min.js"></script>
    <script src="~/Scripts/Kendo_JQ_UI/Vehiculos_libres.js"></script>
    <script src="~/Scripts/ScriptGenerico.js"></script>
}
