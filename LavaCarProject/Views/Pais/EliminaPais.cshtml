@model LavaCarProject.Models.sp_RetornaPais_Result

<h2>Elimina Pais</h2>
<form method="post" id="frmelimnapais">
    <div class="form-group">
        <label>País a eliminar</label>
        <input type="text" class="form-control" name="paiselimina" id="paiselimina" value="@Model.nombre_pais" />
    </div>
    <div class="form-group" role="button">
        <input class="btn btn-danger" type="submit" value="Eliminar" id="btninsertar" />
    </div>
</form>
